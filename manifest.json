{
  "manifest_version": 3,
  "name": "youtube_absolute_time_extension",
  "description": "Show absolute time on YouTube",
  "version": "0.1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ]
}